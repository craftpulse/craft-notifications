{
    "name": "percipioglobal/craft-notifications",
    "description": "Send notifications across a variety of delivery channels, including mail and Slack. Notifications may also be stored in a database so they may be displayed in your web interface.",
    "type": "craft-plugin",
    "version": "1.1.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "notifications"
    ],
    "support": {
        "docs": "https://github.com/percipioglobal/craft-notifications/blob/master/README.md",
        "issues": "https://github.com/percipioglobal/craft-notifications/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Percipio Global Ltd",
            "homepage": "https://percipio.london"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0",
        "ramsey/uuid": "^4.1",
        "guzzlehttp/guzzle": "^6.3|^7.2.0",
        "nesbot/carbon": "^2.19",
        "tightenco/collect": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "percipioglobal\\notifications\\": "src/"
        }
    },
    "extra": {
        "name": "Notifications",
        "handle": "notifications",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/percipioglobal/craft-notifications/master/CHANGELOG.md",
        "components": {
            "notificationsService": "percipioglobal\\notifications\\services\\NotificationsService"
        },
        "class": "percipioglobal\\notifications\\Notifications"
    }
}
